# Database Configuration
DATABASE_URL=postgresql://username:password@host:port/database?sslmode=require
PGHOST=your_postgres_host
PGPORT=5432
PGUSER=your_postgres_user
PGPASSWORD=your_postgres_password
PGDATABASE=your_database_name

# Session Management
SESSION_SECRET=your_random_session_secret_32_characters_or_more

# AI Services
GEMINI_API_KEY=your_google_gemini_api_key_here

# Replit Authentication (automatically set by Replit)
REPL_ID=your_replit_project_id
REPLIT_DOMAINS=your_replit_domain.replit.dev
ISSUER_URL=https://replit.com/oidc

# Supabase Configuration (Optional)
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your_supabase_anonymous_key

# Application Configuration
NODE_ENV=development
PORT=5000

# Example values for development:
# DATABASE_URL=postgresql://user:pass@localhost:5432/nexuslearn
